    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^9.5.3",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.3",
