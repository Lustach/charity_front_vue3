    "@vee-validate/rules": "^4.5.11",
    "@vueuse/core": "^8.2.6",
    "element-plus": "^2.1.8",
    "formvuelate": "^3.8.1",
    "jwt-decode": "^3.1.2",
    "mdi-vue": "^3.0.12",
    "pinia": "^2.0.13",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.4",
    "unplugin-vue-components": "^0.19.0",
    "vee-validate": "^4.5.8",
    "vue": "^3.2.31",
    "vue-axios": "^3.4.1",
    "vue-router": "^4.0.14",
